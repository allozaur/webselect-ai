{
  "manifest_version": 3,
  "name": "WebSelect.ai",
  "version": "0.0.1",
  "description": "Select anything on website and instantly chat with AI",
  "icons": {
    "16": "png/icons/icon16.png",
    "48": "png/icons/icon48.png",
    "128": "png/icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "sidePanel",
    "storage"
  ],
  "host_permissions": [
    "http://localhost/*",
    "http://127.0.0.1/*"
  ],
  "action": {
    "default_icon": {
      "16": "png/icons/icon16.png",
      "48": "png/icons/icon48.png",
      "128": "png/icons/icon128.png"
    },
    "default_title": "WebSelect"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "Command+Shift+1"
      },
      "description": "Toggle WebSelect side panel"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "index.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "key": ""
}